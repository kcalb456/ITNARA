<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="store">
	<select id="item" resultType="Store">
		SELECT * FROM store
		WHERE
		user_id=#{userId}
	</select>

	<select id="product" resultType="Product">
		SELECT * FROM product
		WHERE
		user_id=#{userId}
	</select>
	
	<select id="my" resultType="Store">
	SELECT user_id FROM user WHERE email=#{name}
	</select>
</mapper>